@model List<Service.ViewModel.StudentViewModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Student</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Enrollment Date</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.FirstName
        </td>
        <td>
            @item.LastName
        </td>
        <td>
            @item.EnrollmentDate
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { @*stuid = item.StudentID*@ }) |
            @Html.ActionLink("Details", "Index", "Enrollment", new { stuid = item.StudentID },null)
        </td>
    </tr>
}

</table>
