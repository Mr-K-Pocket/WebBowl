@model DapperService.Model.Student

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Enrollments</h2>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Course</th>
        <th>Grade</th>
    </tr>

    @foreach (var enrollment in Model.Enrollments)
    { 
        <tr>
            <td>
                @Model.FirstName
            </td>
            <td>
                @Model.LastName
            </td>
            <td>
                @enrollment.Title
            </td>
            <td>
                @enrollment.Grade
            </td>
        </tr>
    }

</table>

