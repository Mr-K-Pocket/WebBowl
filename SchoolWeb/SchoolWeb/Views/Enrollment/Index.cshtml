@model List<Service.ViewModel.StudentCourseEnrollment>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Enrollments</h2>

<table class="table">
    <tr>
        <th>Name</th>
        <th>Course</th>
        <th>Credit</th>
        <th>Grade</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.FullName
            </td>
            <td>
                @item.Title
            </td>
            <td>
                @item.Credits
            </td>
            <td>
                @item.Grade
            </td>
        </tr>
    }

</table>

